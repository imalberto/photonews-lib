{
  "name": "photonews-lib",
  "version": "0.0.1",
  "description": "photonews micro lib",
  "dependencies": {
    "debug": "*"
  },
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib",
  "keywords": [
    "express",
    "javascript",
    "modown",
    "mojito",
    "photonews"
  ],
  "peerDependencies": {
    "express": "3.x",
    "express-annotations": "*",
    "express-map": "*",
    "express-state": "*",
    "express-view": "*",
    "express-yui": "~1.0.0",
    "yui": "~3.13.0"
  },
  "devDependencies": {
    "express": "3.x",
    "express-annotations": "*",
    "express-map": "*",
    "express-state": "*",
    "express-view": "*",
    "express-yui": "~1.0.0",
    "yui": "~3.13.0",
    "jshint": "~1.1.0",
    "yuidocjs": ">=0.3.46",
    "yui-lint": "~0.1.3"
  },
  "scripts": {
    "docs": "node ./node_modules/.bin/yuidoc --config ./conf/yuidoc.json ./lib",
    "lint": "node ./node_modules/.bin/jshint --config ./node_modules/yui-lint/jshint.json ./lib/*.js",
    "pretest": "npm run lint"
  },
  "locator": {
    "rulesets": "./conf/rulesets.js"
  },
  "repository": "https://github.com/imalberto/photonews-lib",
  "author": "alberto chan <imalberto@gmail.com>",
  "license": "BSD"
}
